use Modern::Perl 2018;

use CInet::Base;
use CInet::Data::Loader;

use Test::More;
use Test::Deep;

my $lubf4 = CInet::Seq::List->new(
    map { CIR(4 => $_) }
    split /\n/, data_file('LUBF4-list.txt')
);
my $seq = $lubf4 -> map(sub{ $_->minors(3) }) -> flatten
    -> stringify -> uniq;

cmp_deeply [$seq->list], set(
    '011111', '110111', '111101', # L
    '101111', '111011', '111110', # U
    '000011', '001100', '110000', # B
    '000000',                     # F
), 'unique 3-minors of LUBF4 gaussoids';

done_testing;

